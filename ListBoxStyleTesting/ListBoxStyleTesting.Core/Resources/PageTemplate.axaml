<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:vm="clr-namespace:ListBoxStyleTesting.ViewModels">
    <!-- Add Resources Here -->
	<DataTemplate x:Key="PageTemplate" x:DataType="vm:IPage">
		<Grid RowDefinitions="Auto,*">
			<TextBlock Grid.Row="0" Text="{Binding Title}" FontSize="20" Margin="10,4,10,4"/>
			<TextBlock Grid.Row="1" Text="{Binding Content}" FontSize="12" Margin="15,4,15,4"/>
		</Grid>
	</DataTemplate>
</ResourceDictionary>
